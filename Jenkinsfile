pipeline{
    agent none
    stages{
        stage('Init'){
            agent any
            steps{
                echo "Testing ..."
            }
        }
        stage('Build'){
            agent any
            steps{
                echo "Building ..."
            }
        }
        stage('Deploy'){
            agent any
            steps{
                echo "Code Deployed ..."
            }
        }
    }
}
